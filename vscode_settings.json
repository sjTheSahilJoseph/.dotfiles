{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Tokyo Night",
    "editor.fontFamily": "\"Liberation Mono\"",
    "editor.fontLigatures": true,
    "tailwindCSS.emmetCompletions": true,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "git.confirmSync": false,
    "editor.fontVariations": true,
    "editor.minimap.autohide": false,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.settings.useSplitJSON": false,
    "screencastMode.verticalOffset": 0,
    "javascript.format.semicolons": "insert",
    "typescript.format.semicolons": "insert",
    "editor.mouseWheelZoom": true,
    "terminal.integrated.tabs.enableAnimation": false,
    "editor.cursorBlinking": "blink",
    "workbench.tips.enabled": false,
    "screencastMode.fontSize": 40,
    "screencastMode.keyboardOptions": {
        "showCommandGroups": true
    },
    "screencastMode.keyboardOverlayTimeout": 500,
    "explorer.compactFolders": false,
    "extensions.autoUpdate": false,
    "window.customTitleBarVisibility": "auto",
    "editor.lineNumbers": "off",
    "terminal.integrated.cursorStyle": "block",
    "terminal.integrated.cursorStyleInactive": "none",
    "terminal.integrated.cursorBlinking": true,
    "workbench.startupEditor": "none",
    "terminal.integrated.fontFamily": "Liberation Mono",
    "editor.fontSize": 17,
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.integrated.fontSize": 17,
    "editor.cursorStyle": "block",
    "liveServer.settings.donotVerifyTags": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.bracketSameLine": true,
    "prettier.enableDebugLogs": true,
    "prettier.experimentalTernaries": true,
    "prettier.requireConfig": true,
    "prettier.singleAttributePerLine": true,
    "prettier.tabWidth": 4,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.wordWrap": "off",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingComments": "never",
    "editor.autoClosingDelete": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": false,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.guides.indentation": false,
    "editor.guides.highlightActiveIndentation": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.minimap.enabled": false,
    "editor.codeLens": false,
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.autoIndent": "full",
    "editor.cursorWidth": 0,
    "editor.folding": false,
    "editor.tabSize": 4,
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.selectionHighlight": true,
    "workbench.statusBar.visible": true,
    "workbench.editor.alwaysShowEditorActions": false,
    "workbench.editor.editorActionsLocation": "default",
    "workbench.list.smoothScrolling": false,
    "workbench.reduceMotion": "on",
    "editor.hover.enabled": true,
    "editor.tabCompletion": "on",
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": true,
    "editor.renderFinalNewline": "off",
    "editor.renderLineHighlightOnlyWhenFocus": false,
    "prettier.arrowParens": "always",
    "prettier.bracketSpacing": true,
    "prettier.jsxSingleQuote": false,
    "prettier.semi": true,
    "prettier.printWidth": 80,
    "prettier.useTabs": false,
    "prettier.trailingComma": "all",
    "prettier.singleQuote": false,
    "prettier.quoteProps": "as-needed",
    "prettier.withNodeModules": false,
    "python.analysis.autoIndent": true,
    "python.createEnvironment.contentButton": "show",
    "editor.hover.above": true,
    "editor.cursorSurroundingLines": 8,
    "editor.cursorSurroundingLinesStyle": "all",
    "breadcrumbs.enabled": false,
    "workbench.sideBar.location": "left",
    "window.menuBarVisibility": "hidden",
    "window.titleBarStyle": "custom",
    "editor.semanticHighlighting.enabled": false,
    "editor.occurrencesHighlight": "off",
    "editor.foldingHighlight": false,
    "editor.matchBrackets": "never",
    "editor.codeActions.triggerOnFocusChange": false,
    "explorer.autoReveal": false,
    "extensions.autoCheckUpdates": false,
    "editor.snippets.codeActions.enabled": false,
    "problems.autoReveal": false
}


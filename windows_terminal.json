            "brightWhite": "#C6C6C6",
            "brightYellow": "#FF9900",
            "cursorColor": "#26D726",
            "cyan": "#1AFC0E",
            "foreground": "#C6C6C6",
            "green": "#1AFC0E",
            "name": "Love Is An Illusion",
            "purple": "#881798",
            "red": "#FF0000",
            "selectionBackground": "#AA0000",
            "white": "#C6C6C6",
            "yellow": "#FF9900"
        },
    ],
    "startOnUserLogin": false,
    "theme": "system",
    "themes": []
